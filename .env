# .env

# Execution
SESSION_BACKEND=local
EXECUTION_BACKEND=local
FLASK_DEBUG=true
AUTH_DEBUG=true

# BFF auth — pick any string, must match what you send in headers
BFF_SHARED_SECRET=dev-secret

# Data directory — where sessions are stored on disk
BASE_DATA_DIR=./data/sessions

# Flask
HOST=0.0.0.0
PORT=5000